{
  "name": "dbdapt",
  "version": "1.0.0",
  "description": "Лесников Михаил Олегович",
  "main": "index.js",
  "type": "module",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "prisma": "prisma",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "prebuild": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/n3wbieatit-group/DBDaPT.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/n3wbieatit-group/DBDaPT/issues"
  },
  "homepage": "https://gitlab.com/n3wbieatit-group/DBDaPT#readme",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yamls}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "undefined": "^0.1.0"
  }
}
